<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
	<action-mappings>
		<action path="/beanwrite"
				type="org.zjh.struts.BeanWriteTestAction">
			<forward name="success" path="/bean_write.jsp" />
		</action>
		<action path="/empty_present"
				type="org.zjh.struts.EmptyPresentTestAction">
			<forward name="success" path="/empty_present.jsp" />
		</action>
		<action path="/iterate"
				type="org.zjh.struts.IterateTestAction">
			<forward name="success" path="/iterate.jsp" />
		</action>
	</action-mappings>
	<message-resources parameter="MessageResources" />
</struts-config>

